<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Multi"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            margin="{Boolean}false"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <section
                jcr:primaryType="nt:unstructured"
                jcr:title="Section"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <section
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gss-commons/components/dialog/select"
                                class="cq-dialog-dropdown-tab-showhide"
                                cq-dialog-dropdown-tab-showhide-target=".list-option-tab-showhide-target"
                                fieldLabel="Number of section"
                                name="./section">
                                <items jcr:primaryType="nt:unstructured">
                                    <_x0031_
                                        jcr:primaryType="nt:unstructured"
                                        text="1"
                                        value="{Decimal}1"/>
                                    <_x0032_
                                        jcr:primaryType="nt:unstructured"
                                        text="2"
                                        value="{Decimal}2"/>
                                    <_x0033_
                                        jcr:primaryType="nt:unstructured"
                                        text="3"
                                        value="{Decimal}3"/>
                                    <_x0034_
                                        jcr:primaryType="nt:unstructured"
                                        text="4"
                                        value="{Decimal}4"/>
                                    <_x0036_
                                        jcr:primaryType="nt:unstructured"
                                        text="6"
                                        value="{Decimal}6"/>
                                </items>
                            </section>
                        </items>
                    </column>
                </items>
            </section>
            <styles
                jcr:primaryType="nt:unstructured"
                jcr:title="Styles"
                sling:resourceType="granite/ui/components/coral/foundation/include"
                path="/apps/gss-commons/components/dialog/styles"/>
        </items>
    </content>
</jcr:root>
